{
  "name": "dwcsj/peer-tutoring",
  "description": "DWCSJ Peer e-Tutoring System",
  "type": "project",
  "require": {
    "php": "^8.1.0",
    "laravel/framework": "^10.0",
    "barryvdh/laravel-dompdf": "^2.0",
    "laravel/sanctum": "^3.3",
    "spatie/laravel-ignition": "^2.9"
  },
  "require-dev": {
    "fakerphp/faker": "^1.9.1",
    "nunomaduro/collision": "^7.12",
    "laravel/breeze": "^1.29",
    "laravel/tinker": "^2.10",
    "phpunit/phpunit": "10"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "classmap": [
      "database/seeders",
      "database/factories"
    ]
  },

  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
